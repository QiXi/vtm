<?xml version="1.0" encoding="UTF-8"?>
<rendertheme xmlns="http://mapsforge.org/renderTheme" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://mapsforge.org/renderTheme ../renderTheme.xsd" version="1"
map-background="#fffcfa">

  <style-text name="road" k="name" font-size="16" fill="#101010" stroke="#eeeeee"
  stroke-width="2.0" priority="2" />
  <style-text name="major-road" k="name" font-style="bold" font-size="16" fill="#101010"
  stroke="#eeeeee" stroke-width="2.0" priority="1" />
  <style-text name="park" k="name" font-size="17" fill="#93ba8b" stroke="#eeeeee"
  stroke-width="2.0" priority="1" caption="true" />
  <style-text name="poi" k="name" font-size="16" fill="#777744" stroke="#aaffffff"
  stroke-width="2.0" dy="16" caption="true" />

  <!--<style-area name="residential" fill="#e0dfde" fade="10"/> -->
  <style-area name="residential" fill="#e8e7e3" fade="11" />
  <style-area name="railway|industrial" fill="#e6e3e5" fade="10" />
  <!-- "#f0fae7" mint green -->
  <!-- <style-area name="farmland" fill="#fff8bf" fade="11" /> -->
  <!--<style-area name="farmland" fill="#f1ebb2" fade="11" /> -->
  <style-area name="farmland" fill="#e7dab5" fade="11" />
  <!-- fade out at z=7, blend over to 'blend-fill' in z=11 -->
  <style-area name="wood" fill="#d1dbc7" fade="7" blend="11" blend-fill="#9ac56e" />
  <!--<style-area name="wood" fill="#d1dbc7" fade="7" blend="10" blend-fill="#9ac56e" src="grass3.png"/> -->
  <style-line name="wood" fix="true" cap="butt" width="1.0" stroke="#9ac56e" />

  <!-- grass|meadow|garden|grassland|scrub -->
  <style-area name="greens" fill="#c9dc91" fade="10" />
  <style-area name="lightgreen" fill="#d3dcb9" fade="10" />
  <style-area name="darkgreen" fill="#c2cba5" fade="10" />

  <!-- <style-area name="greens" fill="#d2e5ce" fade="10" /> -->
  <!-- marsh|wetland|nature_reserve -->
  <style-area name="greens2" fill="#deecb9" fade="12" />

  <!-- park|common|green|cemetery|golf_course|dog_park -->
  <!-- <style-area name="park" fill="#a3ca7b" fade="11" src="grass2.png" /> -->
  <style-area name="park" fill="#a3ca7b" fade="11" />
  <style-line name="park" stroke="#a3ca7b" width="1.0" fix="true" cap="butt"
  fade="11" />

  <!-- de:Kleingartengebiet -->
  <style-area name="allotments" fill="#efeae0" fade="12" />

  <!-- de:Steinbruch, Schotter-, Kies-, Sand- und Tongrube -->
  <style-area name="quarry" fill="#ddddcc" fade="10" />
  <style-area name="military" fill="#eeedea" fade="10" />
  <style-line name="residential" stroke="#ffffff" width="1.3" />
  <style-line name="residential:bridge" from="residential" cap="square" />

  <!-- when inheriting another style with 'from' then 'witdth' is relative to the parent -->
  <style-line name="pedestrian" from="residential" width="-0.4" />
  <style-line name="pedestrian:bridge" from="pedestrian" cap="square" />
  <style-line name="highway:z11" stroke="#fcba5a" width="1.8" />
  <!-- <style-line name="highway:z11:bridge" from="highway:z11" cap="butt" /> -->
  <style-line name="trunk_link" stroke="#fee16e" width="1.3" cap="butt" />
  <style-line name="trunk" stroke="#fedb52" width="1.6" />
  <style-line name="primary:z11" stroke="#fdeb05" width="1.5" />
  <style-line name="secondary:z11" from="primary:z11" width="-0.1" />
  <style-line name="tertiary" from="residential" stroke="#fefefa" width="0.2" />
  <style-line name="construction" stroke="#e0e0e0" width="1.2" />
  <style-line name="highway-service" from="residential" width="-0.6" />

  <!-- track|footway|path|cycleway -->
  <style-line name="footway" stroke="#aaffffff" width="1.8" cap="butt" fix="true"
  stipple="2" stipple-width="0.6" stipple-stroke="#d35c48" />
  <style-line name="highway:cycleway" from="footway" width="0.1"
  stipple-stroke="#4040ee" />
  <style-line name="highway:track" from="footway" width="0.1" stipple="3"
  stipple-stroke="#a39b68" />
  <style-line name="highway:path" from="footway" width="0.1" stipple="2"
  stipple-stroke="#837b58" />

  <!-- <style-line name="footway:z16" from="footway" width="-0.95" fixed="false" fade="-1"/> -->
  <style-line name="footway:z17" stroke="#faf8f5" width="0.3" />

  <!-- de: ein Weg der fÃ¼r Reiter vorgeshen ist.. -->
  <style-line name="bridleway" from="footway" stroke="#d3cb98" width="0.1" cap="butt"
  stipple-stroke="#837b58" />
  <style-line name="steps" stroke="#aaffffff" width="4.2" cap="butt" fix="true"
  stipple="2" stipple-width="1.0" stipple-stroke="#be6253" />
  <style-line name="water:outline" stroke="#a4bbcc" width="1.0" fix="true" cap="butt" />
  <style-line name="water" stroke="#a4bbcc" width="1.0" cap="butt" />

  <!--<style-area name="water" fill="#97b7e5" afc5e3 /> -->
  <style-area name="water" fill="#99badf" src="water2.png" />

  <!-- no-go area boundary -->
  <style-line name="fence" stroke="#444444" width="1.2" fix="true" cap="butt" />
  <style-line name="aeroway:runway" stroke="#c8ccbe" width="1.8" cap="butt" />

  <!-- <style-line name="building" stroke="#c9c3c1" width="1.0" fix="true" cap="butt" fade="15"/> -->
  <!-- <style-line name="building" stroke="#d0cec8" width="1.0" fix="true" cap="butt" fade="15"/>
  <style-area name="building" fill="#e9e6e3" fade="15"/> -->
  <style-line name="building" stroke="#b7b6b3" width="1.0" fix="true" cap="butt"
  fade="15" />
  <!-- <style-area name="building" fill="#eae8e6" fade="15"/> -->
  <style-area name="building" fill="#f2f0eb" fade="15" />
  <atlas img="osm.png">
    <rect name="airport" pos="1 40 16 16"/>
    <rect name="atm" pos="19 40 16 16"/>
    <rect name="bar" pos="1 80 20 20"/>
    <rect name="cafe" pos="67 88 16 16"/>
    <rect name="camping" pos="71 110 16 16"/>
    <rect name="cinema" pos="1 102 24 24"/>
    <rect name="city" pos="55 11 10 10"/>
    <rect name="department_store" pos="67 70 16 16"/>
    <rect name="fast_food" pos="67 52 16 16"/>
    <rect name="florist" pos="67 34 16 16"/>
    <rect name="food_bar" pos="1 22 16 16"/>
    <rect name="food_drinkingtap" pos="23 80 20 20"/>
    <rect name="hospital" pos="19 22 16 16"/>
    <rect name="hostel" pos="27 106 20 20"/>
    <rect name="hotel" pos="45 84 20 20"/>
    <rect name="information" pos="37 22 16 16"/>
    <rect name="library" pos="49 106 20 20"/>
    <rect name="museum" pos="67 16 16 16"/>
    <rect name="parking" pos="1 4 16 16"/>
    <rect name="pharmacy" pos="19 4 16 16"/>
    <rect name="place_of_worship" pos="37 4 16 16"/>
    <rect name="playground" pos="45 62 20 20"/>
    <rect name="police" pos="85 92 16 16"/>
    <rect name="pub" pos="89 110 16 16"/>
    <rect name="rental_bicycle" pos="1 58 20 20"/>
    <rect name="restaurant" pos="85 74 16 16"/>
    <rect name="school" pos="85 56 16 16"/>
    <rect name="shop_bakery" pos="85 38 16 16"/>
    <rect name="shop_butcher" pos="85 20 16 16"/>
    <rect name="shop_clothes" pos="85 2 16 16"/>
    <rect name="shop_convenience" pos="103 92 16 16"/>
    <rect name="shop_diy" pos="107 110 16 16"/>
    <rect name="shop_hairdresser" pos="103 74 16 16"/>
    <rect name="shop_supermarket" pos="103 56 16 16"/>
    <rect name="shopping_bicycle" pos="103 38 16 16"/>
    <rect name="telephone" pos="103 20 16 16"/>
    <rect name="theatre" pos="23 58 20 20"/>
    <rect name="toilets" pos="45 40 20 20"/>
    <rect name="town" pos="67 3 11 11"/>
    <rect name="view_point" pos="103 2 16 16"/>
    <rect name="volcano" pos="55 1 9 8"/>
    <rect name="windmill" pos="55 23 10 15"/>
  </atlas>

  <!-- ways -->
  <m e="way">

    <!-- landuse base -->
    <m k="landuse">
      <m v="urban">
        <area fill="#f4f3f0" />
      </m>
      <m v="residential|commercial|retail">
        <use-area name="residential" />
      </m>
    </m>
    <m k="natural" v="grassland|scrub" zoom-min="10">
      <use-area name="darkgreen" />
    </m>
    <m k="landuse">
      <m zoom-min="10" v="farmland|farm|orchard|vineyard|greenhouse_horticulture|plant_nursery">
        <use-area name="farmland" />
      </m>
      <m v="quarry">
        <use-area name="quarry" />
      </m>
      <!-- <m v="farmyard|retail|commercial|industrial;retail">
      <use-area name="residential" />
      </m> -->
      <m v="industrial|railway">
        <use-area name="railway|industrial" />
      </m>
      <!-- <m v="military">
      <use-area name="military"/>
      </m> -->
      <!--<m k="landuse" v="construction|greenfield"> <area fill="#a47c41"
      stroke="#e4e4e4" width="0.2" /> </m> -->
      <!-- <m k="landuse" v="garages"> <area fill="#d6d6e4" /> </m> -->
    </m>
    <m k="landuse|natural|leisure||amenity|tourism">
      <!-- kind of more like landuse imho -->
      <m k="leisure|landuse" v="nature_reserve">
        <use-area name="greens2" />
        <m zoom-min="14">
          <line stroke="#abe29c" width="1.0" fix="true" cap="butt" />
        </m>
      </m>

      <!-- tourism areas
      Berlin Zoologischer Garten has lots of details that should be drawn above
      -->
      <m k="tourism">
        <!-- <m k="tourism" v="attraction"> <area fill="#f2caea" /> </m> -->

        <m v="zoo|picnic_site|caravan_site|camp_site">
          <area fill="#c0d69a" />
        </m>
      </m>

      <!-- amenity -->
      <m k="amenity" zoom-min="14">
        <m v="kindergarten|school|college|university">
          <!-- <area fill="#cdabde" /> -->
          <area fill="#e6e4c5" fade="14" />
          <line stroke="#9aabae" width="1.0" fix="true" cap="butt" fade="14" />
        </m>
        <m v="hospital">
          <area fill="#f2d9b1" />
        </m>
        <!-- <m v="parking" zoom-min="15">
        <area fill="#f4f4f4" stroke="#d4d4d4" stroke-width="0.2" />
        </m>
        <m v="fountain" closed="yes">
        <area fill="#b4cbdc" stroke="#000080" stroke-width="0.15" />
        </m> -->
      </m>

      <!-- landuse -->
      <m k="landuse" zoom-min="11">
        <!-- how about 'leisure' for this one? -->
        <m v="cemetery">
          <use-area name="park" />
        </m>
      </m>
      <m k="landuse" v="village_green|recreation_ground">
        <use-area name="greens" />
      </m>
      <m k="landuse" v="allotments" zoom-min="12">
        <use-area name="allotments" />
      </m>
      <m k="leisure" v="park|common|green|golf_course" zoom-min="11">
        <use-area name="park" />
        <m zoom-min="14">
          <use-line name="park" />
        </m>
        <!-- <use-text name="park" /> -->
      </m>

      <!-- Heideland, keep below forest -->
      <m zoom-min="11" v="heath|sand">
        <area fill="#fffad1" fade="11" />
      </m>
      <m k="landuse|natural" v="forest|wood">
        <use-area name="wood" />
        <m zoom-min="14">
          <use-line name="wood" />
        </m>
      </m>

      <!-- keep grass above forest:wood and leisure:park! -->
      <!-- http://wiki.openstreetmap.org/wiki/Proposed_features/conservation,
      often serves as background for leisure=nature_reserve -->
      <m k="landuse" v="grass">
        <use-area name="lightgreen" />
      </m>
      <m k="landuse" v="meadow|conservation">
        <use-area name="greens" />
      </m>
      <m k="leisure" v="garden">
        <use-area name="greens" />
      </m>

      <!-- amenity -->
      <m k="amenity">
        <!--<m v="kindergarten|school|college|university" zoom-min="15">
        <area fill="#cdbbca" fade="15"/>
        <line stroke="#9aabae" width="1.0" fix="true" cap="butt" fade="15"/>
        </m>

        <m v="hospital" zoom-min="14">
        <area fill="#e6e4c5" />
        </m>
        -->
        <m v="parking" zoom-min="15">
          <area fill="#f4f4f4" stroke="#d4d4d4" stroke-width="0.2" />
        </m>
        <m v="fountain" closed="yes">
          <area fill="#b4cbdc" stroke="#000080" stroke-width="0.15" />
        </m>

      </m>

      <!-- <m k="natural" v="coastline">
      <line stroke="#a4bbcc" width="1.2" fix="true" />
      </m> -->

      <!-- natural -->
      <m k="natural" zoom-min="10">
        <m v="glacier">
          <area fill="#fafaff" />
        </m>
        <m v="land">
          <area fill="#f8f8f8" />
        </m>
        <m v="beach">
          <area fill="#f7f5c8" />
        </m>
        <m v="marsh|wetland|mud">
          <use-area name="greens2" />
        </m>
      </m>

      <!-- leisure -->
      <m k="leisure" zoom-min="13">

        <m v="stadium">
          <line stroke="#c9c3c1" width="1.0" fix="true" cap="butt" />
          <area fill="#e9e6e3" />
        </m>

        <!--should be under playing field -->
        <m zoom-min="14" v="sports_centre|water_park">
          <area fill="#daefdb" fade="12" />
        </m>
        <m zoom-min="15" v="playground|miniature_golf">
          <area fill="#f4f4de" />
          <line stroke="#d9d9a3" width="1.0" fix="true" cap="butt" />
        </m>
        <m v="playing_fields|pitch">
          <area fill="#f4f4de" />
          <line stroke="#d9d9a3" width="1.0" fix="true" cap="butt" />
        </m>
        <m v="swimming_pool">
          <area fill="#d4ebfc" stroke="#6060ff" stroke-width="0.2" />
        </m>

        <!-- <m v="track"> <m k="area" v="yes|true">
        <area fill="#c9d8a2" stroke="#b7c690" width="0.025" /> </m> <m
        e="way" k="area" v="~|no|false"> <line stroke="#b7c690" width="0.75"
        /> </m> </m> -->
      </m>

      <!-- area outlines need to be above to avoid uggly pixelation where
      not aliased polygon overlaps the lines... -->
      <m k="leisure|landuse" zoom-min="14">

        <m v="nature_reserve">
          <line stroke="#abe29c" width="1.0" fix="true" cap="butt" />
        </m>
        <m v="military">
          <use-line name="fence" />
        </m>
      </m>
      <m k="landuse" v="reservoir|basin">
        <use-area name="water" />
      </m>

      <!-- ...should rewrite tag to: highway=leisure/sport=* imho -->
      <m k="leisure" v="track">
        <line stroke="#c1bcb6" width="1.3" cap="butt" fix="true" />
      </m>

    </m><!-- end landuse|natural|leisure||amenity|tourism -->

    <!-- waterways -->
    <m k="waterway">
      <m v="ditch|drain" zoom-min="14">
        <use-line name="water" width="0.2" fix="true" />
      </m>
      <m v="canal">
        <use-line name="water" width="-0.3" />
      </m>
      <m v="stream" zoom-min="14">
        <use-line name="water" width="0.5" fix="true" />
      </m>
      <m v="river">
        <m zoom-min="12">
          <use-line name="water" width="0.3" />
        </m>
        <m zoom-max="11">
          <m k="rank" v="0|1|2">
            <use-line name="water" width="0.3" fix="true" fade="2" />
          </m>
          <m k="rank" v="3" zoom-min="3">
            <use-line name="water" width="0.3" fix="true" fade="3" />
          </m>
          <m k="rank" v="4" zoom-min="4">
            <use-line name="water" width="0.3" fix="true" fade="4" />
          </m>
          <m k="rank" v="5" zoom-min="5">
            <use-line name="water" width="0.3" fix="true" fade="5" />
          </m>
          <m k="rank" v="6" zoom-min="6">
            <use-line name="water" width="0.2" fix="true" fade="6" />
          </m>
          <m k="rank" v="7" zoom-min="7">
            <use-line name="water" width="0.2" fix="true" fade="7" />
          </m>
          <m k="rank" v="8" zoom-min="8">
            <use-line name="water" width="0.1" fix="true" fade="8" />
          </m>
          <m k="rank" v="9" zoom-min="9">
            <use-line name="water" width="0.1" fix="true" fade="9" />
          </m>
          <m k="rank" v="-1" zoom-min="9">
            <use-line name="water" width="0.2" fix="true" fade="9" />
          </m>
        </m>
      </m>
      <m v="riverbank|dock">
        <use-area name="water" />
        <use-line name="water:outline" />
      </m>
      <m v="weir">
        <line stroke="#000088" width="1.0" fix="true" cap="butt" />
      </m>
      <m v="dam" zoom-min="12">
        <!-- sehr seltsam was in deutschland so als Damm gekenzeichnet wird..
        siehe Muenster -->
        <line stroke="#ababab" width="1.2" cap="butt" fix="true" />
      </m>
      <m k="lock" v="yes|true">
        <line stroke="#f8f8f8" width="1.5" cap="butt" />
      </m>
    </m>
    <m k="natural" v="water">
      <use-area name="water" />
      <use-line name="water:outline" />
    </m>

    <!-- sport -->
    <!-- <m k="sport"> <m k="sport" v="soccer"
    zoom-min="17"> <m k="sport" v="swimming|canoe|diving|scuba_diving">
    <area fill="#b4cbdc" stroke="#6060ff" width="0.2" /> </m> <m
    e="way" k="sport" v="tennis"> <area fill="#d18a6a" stroke="#b36c4c" width="0.2"
    /> </m> </m> -->

    <!-- outline 0 -->
    <style-outline name="glow" stroke="#000000" width="0.2" blur="1.0" />
    <style-outline name="0" stroke="#44000000" width="0.1" />
    <!-- tunnel -->
    <m k="tunnel" v="yes" zoom-min="11">
      <!-- highway tunnels -->
      <m k="highway">
        <m zoom-min="16">
          <m v="steps">
            <use-line name="steps" />
            <!-- <use-outline name="0"/> -->
          </m>
          <!-- <m v="track|footway|path|cycleway" zoom-min="16">
          <use-line name="footway:z16"/>
          </m> -->
        </m>
        <m zoom-min="14">
          <m v="footway">
            <use-line name="footway" />
          </m>
          <m v="cycleway">
            <use-line name="highway:cycleway" />
          </m>
          <m v="track">
            <use-line name="highway:track" />
          </m>
          <m v="path">
            <use-line name="highway:path" />
          </m>
          <m v="bridleway">
            <use-line name="bridleway" />
          </m>
          <m v="construction">
            <use-line name="construction" />
            <use-outline name="0" />
          </m>
          <m v="service">
            <m k="service" v="~|parking_aisle">
              <use-line name="highway-service" />
              <use-outline name="0" />
            </m>
          </m>
        </m>
        <m v="trunk_link|motorway_link">
          <use-line name="trunk_link" />
          <use-outline name="0" />
          <use-text name="major-road" />
        </m>
        <m zoom-min="13">
          <m v="byway|pedestrian">
            <use-line name="pedestrian" />
            <use-outline name="0" />
            <use-text name="road" />
          </m>
          <m v="residential|road|unclassified|living_street">
            <use-line name="residential" />
            <use-outline name="0" />
            <use-text name="road" />
          </m>
        </m>
        <m v="tertiary|secondary_link">
          <use-line name="tertiary" />
          <use-outline name="0" />
          <use-text name="road" />
        </m>
        <m v="secondary|primary_link">
          <use-line name="secondary:z11" />
          <use-outline name="0" />
          <use-text name="major-road" />
        </m>
        <m v="primary">
          <use-line name="primary:z11" />
          <use-outline name="0" />
          <use-text name="major-road" />
        </m>
        <m v="trunk">
          <use-line name="trunk" blur="0.3" />
          <use-outline name="0" />
          <!-- <use-outline name="glow"/> -->
          <use-text name="major-road" />
        </m>
        <m v="motorway">
          <use-line name="highway:z11" blur="0.3" />
          <use-outline name="0" />
          <!-- <use-outline name="glow"/> -->
          <use-text name="major-road" />
        </m>
      </m>

      <!-- railway tunnel -->
      <m k="railway">
        <!-- <m k="railway" v="tram|subway|light_rail|narrow_gauge">
        <line stroke="#a0a0a0" width="0.8" cap="butt" fix="true"/>
        </m> -->
        <m k="railway" v="rail">
          <line stroke="#aa888888" width="0.9" cap="butt" fix="true" />
        </m>
      </m>
    </m><!-- end tunnel -->

    <!-- platform cores -->
    <m k="highway|railway|public_transport" v="platform">
      <m closed="yes">
        <area fill="#dbdbc9" />
      </m>
      <m closed="no">
        <line stroke="#dbdbc9" width="0.3" />
      </m>
    </m>

    <!-- runways areas -->
    <m k="aeroway">
      <m v="aerodrome" closed="yes">
        <area fill="#e8ecde" />
      </m>
      <!-- A place where planes are parked -->
      <m v="apron">
        <area fill="#f0f0f0" />
      </m>
      <!-- Airport passenger building -->
      <m v="terminal|hangar">
        <use-line name="building" />
        <use-area name="building" />
      </m>
    </m>

    <!-- turning circles -->
    <!-- <m k="highway" v="turning_circle"> <circle r="1.5" scale-radius="true"
    fill="#707070" />
    </m> -->

    <!-- building -->
    <m k="building">

      <m zoom-min="14" zoom-max="16">
        <m closed="yes">
          <use-area name="building" fade="14" />
        </m>
        <use-line name="building" fade="14" />
        <!-- <line stroke="#c9c3c1" width="1.0" fix="true" cap="butt" fade="15"/>
